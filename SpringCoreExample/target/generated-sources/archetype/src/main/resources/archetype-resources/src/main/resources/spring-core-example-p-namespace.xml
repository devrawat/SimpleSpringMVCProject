#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:p="http://www.springframework.org/schema/p"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">
    <bean id="calculateInterest" class="${package}.core.p.namespace.CalculateInterest"
        p:loan="154000"
        p:durationInMonths="36"
        p:interestInfo-ref="inetrestInfo" />
    <bean id="inetrestInfo"
        class="${package}.core.p.namespace.InterestInformation"
        p:interestRate="3.5"
        p:interestType="Simple" />
</beans>
